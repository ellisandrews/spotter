<%= render partial: 'messages/model_errors', locals: { model: scheduled_workout } %>

<%= form_with model: scheduled_workout, local: true do |f| %>

  <%= f.label :date %>
  <%= f.date_select :date %>

  <br>

  <%= f.label :workout_id %>
  <%= f.collection_select :workout_id, workouts, :id, :name, prompt: true %>

  <br>

  <%= f.label :completed %>
  <%= f.check_box :completed %>

  <br><br>

  <%= f.submit 'Schedule' %>

<% end %>
